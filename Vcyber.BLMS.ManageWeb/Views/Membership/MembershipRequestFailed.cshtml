@{
    Layout = null;
}
<style>
    body {
        display: none;
    }
</style>

<script>

    $(function () {
        if (self == top) {
            var theBody = document.getElementsByTagName('body')[0];
            theBody.style.display = "block";
        } else {
            top.location = self.location;
        }
    });


</script>
<div class="container panel panel-default">
    <div class="panel-body"><strong>用户管理 -银卡会员认证失败列表 </strong></div>
</div>
<div class="panel panel-default">
    <div class="panel-heading">

        <div class="row">
            <span class="col-md-6">银卡会员认证失败列表</span>
            
        </div>
    </div>
    <div class="panel-body">
        <form name="form" class="form-horizontal" novalidate>
            <div class="form-group">
                <label class="col-md-2 control-label">处理状态：</label>
                <div class="col-md-2 text-left">
                    <select ng-model="selected" class="form-control" ng-options="m.id as m.Name for m in statuslist">
                        <option value="">请选择</option>
                    </select>
                </div>

                <label class="col-md-2 control-label">手机号：</label>
                <input class="col-md-2 form-control" type="text" ng-model="phoneNumber" />
          
                <div class="col-md-2">
                    <button class="btn btn-primary" type="submit" ng-click="search()">查询</button>
                    <a class="btn btn-primary" href="/Membership/ExportMembershipFailedData?clState={{selected}}" target="_blank" >导出</a>
                </div>
            </div>

        </form>
        <div class="gridStyle" style="min-height: 410px; border: 1px solid rgb(212, 212, 212)" ng-grid="gridOptions">
        </div>
    </div>
</div>
<div class="modal fade" id="IdentityNumberModal" tabindex="-1" role="dialog" aria-labelledby="IdentityNumberModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="IdentityNumberModal">编辑身份证</h4>
            </div>
            <form class="form-horizontal" name="form" ng-submit="updateIdentityNumber()" novalidate>
                <div class="modal-body">
                    <div class="row">

                        <div class="form-group" >
                            <label class="col-md-2 control-label"><span style="color:red">*</span>身份证:</label>
                            <div class="col-md-10">
                                <input class="form-control" name="identityNumber" ng-model="selectItem.IdentityNumber" ng-pattern="/^(^\d{15}$)|(^\d{18}$)|(^\d{17}(\d|X|x)$)$/" required />
                                <span ng-show="form.identityNumber.$invalid">不能为空</span>

                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                    <button type="submit" class="btn btn-primary" ng-disabled="form.$invalid">保存</button>
                </div>
            </form>
        </div>
    </div>
</div>
