<script type="text/javascript">
    function checkFaceImgType(obj) {
        if ($(obj).val() == null || $(obj).val() == "") {
            popWindownBlue( "请选择后缀名为.jpg或者.png类型的文件作为头像");
            return false;
        }
        var _index = $(obj).val().length;
        var _extenssionName = $(obj).val().substring(_index - 3);
        if (_index < 0) {
            popWindownBlue( "请选择后缀名为.jpg或者.png类型的文件作为头像");
            return false;
        }
        if (_extenssionName != "jpg" && _extenssionName != "png" && _extenssionName != "PNG" && _extenssionName != "JPG") {
            popWindownBlue( "请选择后缀名为.jpg或者.png类型的文件作为头像");
            return false;
        }
        InitImgUrl();
          $(".user_base_info_upload").click();

    }
    function InitImgUrl() {
        var objUrl = getObjectURL(document.getElementById("file").files[0]);
        if (objUrl) {
            $("#img_upload").attr("src", objUrl);
        }
    }
    function getObjectURL(file) {
        var url = null;
        if (window.createObjectURL != undefined) { // basic
            url = window.createObjectURL(file);
        } else if (window.URL != undefined) { // mozilla(firefox)
            url = window.URL.createObjectURL(file);
        } else if (window.webkitURL != undefined) { // webkit or chrome
            url = window.webkitURL.createObjectURL(file);
        }
        return url;
    }
    function BeforUpload() {
        $("#img_upload").attr("src", "/Img/img_process.gif");
        return true;
    }
    function update_upload_img_src(imgsrc) {
        $("#img_upload").attr("src", imgsrc);
        $("#faceimage").val(imgsrc);
    }
    $(function () {
        $("#img_upload").click(function () {
            $("#file").click();
        })

        var _update_identitynumber = '@Model.IdentityNumber';
        if (_update_identitynumber != null || _update_identitynumber != '') {
            if ($("#setbaseinfoUserPaperWork").val() == null || Number($("#setbaseinfoUserPaperWork").val()) < 1) {
                $("#setbaseinfoUserPaperWork").val(-1);
            }
            $("#setbaseinfoUserPaperWork").attr("disabled", "disabled");
            $("#setbaseinfoUserIdentityNumber").attr("disabled", "disabled");
        }
    })

</script>
