                if (!string.IsNullOrEmpty(Model.IsMarriage))
                {
                    int.TryParse(Model.IsMarriage, out _ismarriage);
                }
            }
            <input type="radio" id="setbaseinfoUserIsMarriageYes" class="radioButton" name="IsMarriage" @{if (_ismarriage == (int)Vcyber.BLMS.Entity.Enum.ECustomerMarriageType.Is) { <text> checked="checked" </text>     }} value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerMarriageType.Is)" />
            <label for="setbaseinfoUserIsMarriageYes" class="radioText">
                已婚
            </label>
            <input type="radio" id="setbaseinfoUserIsMarriageNo" @{if (_ismarriage == (int)Vcyber.BLMS.Entity.Enum.ECustomerMarriageType.No) { <text> checked="checked" </text>       }} class="radioButton" name="IsMarriage" value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerMarriageType.No)" />
            <label for="setbaseinfoUserIsMarriageNo" class="radioText">
                未婚
            </label>

        </p>
        <p>
            <span class="left_property_name">
                <span>&nbsp;</span><label>婚姻纪念日：</label>
            </span>
            <input placeholder="请选择婚姻纪念日 " type="text" id="setbaseinfoUserMarriageDay" name="MarriageDay" value="@(string.IsNullOrEmpty(Model.MarriageDay)?"":Convert.ToDateTime(Model.MarriageDay??"").ToString("yyyy-MM-dd"))" onclick="laydate()" />
        </p>
        <p>
            <span class="left_property_name">
                <span>&nbsp;</span><label>教育程度：</label>
            </span>
            <select id="setbaseinfoUserEducational" name="Educational" class="user_base_info_select">
                <option value="-1">其他</option>
                @{
                    int _educational = -1;
                    if (!string.IsNullOrEmpty(Model.Educational))
                    {
                        int.TryParse(Model.Educational, out _educational);
                    }
                }
                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.University)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.University == _educational)
                                                                                                  { <text> selected="selected" </text>   }}>大学</option>

                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Bachelor)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Bachelor == _educational)
                                                                                                { <text> selected="selected" </text>   }}>学士</option>

                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Master)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Master == _educational)
                                                                                              { <text> selected="selected" </text>   }}>硕士</option>

                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Dr)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Dr == _educational)
                                                                                          { <text> selected="selected" </text>   }}>博士</option>
                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.JuniorCollege)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.JuniorCollege == _educational)
                                                                                                     { <text> selected="selected" </text>    }}>大专</option>
                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Polytechnic)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.Polytechnic == _educational)
                                                                                                   { <text> selected="selected" </text>    }}>中专</option>
                <option value="@((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.MiddleSchool)" @{if ((int)Vcyber.BLMS.Entity.Enum.ECustomerEducational.MiddleSchool == _educational)
                                                                                                  { <text> selected="selected" </text>    }}>高中以下</option>
            </select>
        </p>
        <p class="Remark_Container">
            <span class="left_property_name">
                <span>&nbsp;</span><label>备注：</label>
            </span>
            <textarea id="setbaseinfoUserRemark" name="Remark">@Model.Remark</textarea>
        </p>
