@section AddToFooterJs{
    <script type="text/javascript">
        $('#slider').slider({
            nextBtn: ".next",
            prevBtn: ".prev",
            nums: true,
            numsClass: '.add',
            times: true,
            timer: 5000
        });

        $(".center_lb_title>div").hover(function () {
            $(this).siblings("div").removeClass("active");
            var index = $(".center_lb_title>div").index(this);
            $(this).addClass("active");
            $(".cont-box>div").eq(index).show().siblings('div').hide();

        }, function () {
        });

        $(".tab-title-content").hover(function () {
            if (!$(this).hasClass("tab-active")) {
                $(this).addClass("tab-active");
                $(this).siblings().removeClass("tab-active");
                var _t_i = $(".tab-title-content").index(this);
                $(".tab-content>div").eq(_t_i).show().siblings().hide();
            }
        }, function () {
        });

        $(function () {
            $(".closeIcon").on("click", function () {
                $(".zzDivBox").css('display', 'none');
                window.location = "/MyCenter/Index?pageName=MyCenter";
            });
            $(".zzConBtn").on("click", function () {
                $(".zzDivBox").css('display', 'none');
            });
        });

    </script>
}
