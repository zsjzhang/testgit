@model IEnumerable<string>
<head>
    <style>
        body {
            display: none;
        }
    </style>
    <script>
        try{
            top.location.hostname;
            if (top.location.hostname != window.location.hostname) {
            top.location.href =window.location.href;
        }
        }
            catch(e){
            top.location.href = window.location.href;
        }
    </script>
</head>

<script>
    if (self == top) {
        var theBody = document.getElementsByTagName('body')[0];
        theBody.style.display = "block";
    } else {
        top.location = self.location;
    }
</script>
<div style="width: 100%; float: left; margin: 1em auto;">
    <select id="sltProvince" style="height: 35px; width: 45%; float: left; " onchange="Dealer.city(this);">
        <option value="-1">请选择</option>
        @{
            if (Model != null && Model.Any())
            {
                foreach (var item in Model)
                {
                    <option>@item</option>
                }
            }
        }
    </select>
    <select id="sltCity" style="height: 35px; width: 45%; float: right; " onchange="Dealer.list(this, document.getElementById('sltProvince'));">
        <option value="-1">请选择</option>
    </select>
</div>
<div style="width: 100%; float: left; margin: 1em auto;">
    <select id="sltDealer" style="width: 100%; height:35px;">
        <option value="-1">请选择特约店</option>
    </select>
</div>








