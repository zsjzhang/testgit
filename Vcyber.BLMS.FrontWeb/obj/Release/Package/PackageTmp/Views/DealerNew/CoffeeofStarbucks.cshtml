@{
    Layout = "~/Views/Shared/_LayoutPageHome.cshtml";
    ViewBag.Title = "生活服务-星巴克电子饮品券";
}
<link href="~/css/DealerNew/reset.css" rel="stylesheet" />
<link href="~/css/DealerNew/brand.css" rel="stylesheet" />
<script type="text/javascript">
    $(document).ready(function () {
        $("#btnGotoLogin").click(function () {
            var returnUrl = encodeURIComponent("/DealerNew/CoffeeofStarbucks");
            location.href = "/Account/LogonPage?returnUrl=" + returnUrl;
            return false;
        });
        $("#btnGotoRegister").click(function () {
            var returnUrl = encodeURIComponent("/DealerNew/CoffeeofStarbucks");
            location.href = "/Account/AccountRegister?returnUrl=" + returnUrl;
            return false;
        });


    });
</script>
