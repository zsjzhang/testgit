@model List<Vcyber.BLMS.Common.City.City>
@{
    Layout = null;
}

<select id="myOrderAddressCity" onchange="User.FindAreasByCity(this);" class="select-city">
    <option value="-1">请选择</option>
    @{
        if (Model != null && Model.Any())
        {
            foreach (var item in Model)
            {
                <option value="@item.Code" title="@item.ID">@item.Name</option>
            }
        }
    }
</select>
