@model IEnumerable<string>
@{
    Layout = null;
}

    <label id="ProvinceAndCityProvinceLabel" style="width:30px">省：</label>
    <select id="sltProvince2" onchange="Dealer2.city(this);" style="width:100px">
    <option value="-1">请选择</option>
    @{
        if (Model != null && Model.Any())
        {
            foreach (var item in Model)
            {
                <option>@item</option>
            }
        }
    }
</select>


    
    <label id="ProvinceAndCityCityLabel" style="width: 30px; padding-left:5px">市：</label>
    <select id="sltCity2" style="width:100px" onchange="Dealer2.list(this, document.getElementById('sltProvince2'));">
    <option value="-1">请选择</option>
</select>
<select id="sltDealer2" style="display:none" hidden="true">
    <option value="-1">请选择</option>
</select>



