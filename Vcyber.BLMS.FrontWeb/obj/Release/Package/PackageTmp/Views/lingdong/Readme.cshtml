                    </div>
                    <div class="commonrow">
                        <img src="~/Contents/images/lingdong/j3.png" style="width:20%;" />
                    </div>
                    <div class="commonrow3">
                        <img src="~/Contents/images/lingdong/j4.png" style="height:90px;" />
                        <marquee direction="up" scrollamount="1" class="myarea" id="areaDrivAward1">
                            @{
                                int i = 0;
                            }
                            @foreach (var item in ViewBag.drvAwardList)
                            {
                                if (i > 20)
                                {
                                    break;
                                }
                                if (string.IsNullOrWhiteSpace(item.PhoneNumber) || item.PhoneNumber.Length < 11)
                                {
                                    continue;
                                }
                                string p = item.PhoneNumber.Substring(0, 3) + "****" + item.PhoneNumber.Substring(7, 4) + " ";
                                string s = item.AwardName + " " + p;
                                i++;
                                @p<br />
                            }
                        </marquee>
                        <textarea class="myarea" id="areaDrivAward" disabled="disabled" style="display:none"></textarea>
                    </div>
                    <div class="commonrow">
                        <img src="~/Contents/images/lingdong/j5.png" style="width:20%;" />
                    </div>
