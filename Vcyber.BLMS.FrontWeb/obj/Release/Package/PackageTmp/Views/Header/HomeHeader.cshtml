<script type="text/javascript">
    var mouseover_tid = [];
    var mouseout_tid = [];
    var _cookieHelper = new Cookie();
    (function () {
        if (_cookieHelper.getCookie("rightShortcutsRegion") != null && _cookieHelper.getCookie("rightShortcutsRegion") != "") {
            $("#rightShortcutsRegion").show().siblings("li").hide();
        } else {
            $("#rightShortcutsRegion").siblings("li").show();
        }
    })();

    $(function () {
        //一级导航
        $('#menus>li').each(function (index) {
            $(this).hover(
                // 取消淡出菜单的线程, 延时淡入菜单
                function () {
                    var _self = this;
                    clearTimeout(mouseout_tid[index]);
                    mouseover_tid[index] = setTimeout(function () {
                        $(_self).find('ul:eq(0)').fadeIn(100);
                    }, 10);
                },

                // 取消淡入菜单的线程, 延时淡出菜单
                function () {
                    var _self = this;
                    clearTimeout(mouseover_tid[index]);
                    mouseout_tid[index] = setTimeout(function () {
                        $(_self).find('ul:eq(0)').fadeOut(100);
                    }, 10);
                }

            );
        });

        //二级导航
        $(".leveltwo").find("li").each(function () {
            $(this).hover(
                function () {
                    $(this).addClass("levelTwoNN");
                },
                function () {
                    $(this).removeClass("levelTwoNN");
                }
                );
        });

        //收起
        $("#rightShortcutsRegion").click(function () {
            if (_cookieHelper.getCookie("rightShortcutsRegion") != null && _cookieHelper.getCookie("rightShortcutsRegion") != "") {
                $(this).siblings("li").show();
                _cookieHelper.setCookie("rightShortcutsRegion", "");
                return false;
            } else {
                $(this).show().siblings("li").hide();
                _cookieHelper.setCookie("rightShortcutsRegion", "isshowregion;path=/");
                return false;
            }
        });
    });
</script>
