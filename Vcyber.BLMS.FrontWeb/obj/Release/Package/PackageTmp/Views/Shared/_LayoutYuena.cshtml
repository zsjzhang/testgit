    <meta name="viewport" content="width=device-width" />
    <meta name="baidu-site-verification" content="qSbMsfRL2A" />
    <title>@ViewBag.Title</title>
    <!--悦纳活动JS文件结束-->
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Content/layer/layer.min.js"></script>
    <script src="~/Scripts/base/silder.js"></script>
    <script src="~/Scripts/base/js.js"></script>
    <script src="~/Scripts/controller/Main.js"></script>
    <script src="~/Scripts/controller/UserController.js"></script>
    <script src="~/Scripts/controller/CarController.js"></script>

    <!--悦纳活动样式-->
